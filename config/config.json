{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "azure-blob-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/azureBlob/AzureBlobWebPart.js",
          "manifest": "./src/webparts/azureBlob/AzureBlobWebPart.manifest.json"
        }
      ]
    },
    "user-validation-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/userValidation/UserValidationWebPart.js",
          "manifest": "./src/webparts/userValidation/UserValidationWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "AzureBlobWebPartStrings": "lib/webparts/azureBlob/loc/{locale}.js",
    "UserValidationWebPartStrings": "lib/webparts/userValidation/loc/{locale}.js"
  }
}
